{
  "extends": [ 
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "no-console": 0,
      "no-unused-vars": [
        "error", { 
          "vars": "all", 
          "args": "after-used", 
          "ignoreRestSiblings": false 
        }
      ],
      "react/prop-types": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ]
}
